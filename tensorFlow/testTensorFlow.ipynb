    "import tensorflow as tf\n",
    "a = tf.constant([1.0, 2.0], name = \"a\")\n",
    "b = tf.constant([2.0, 3.0], name = \"b\")\n",
    "result = a + b\n",
    "sess = tf.InteractiveSession()  #生成一个会话\n",
    "print(result.eval())\n",
    "sess.close()     #释放计算资源\n",
    "with tf.Session() as sess:\n",
    "    print(sess.run(result))"
   ]
